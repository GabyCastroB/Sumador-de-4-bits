module testbench;

  // Inputs
  reg [3:0] xi;
  reg [3:0] yi;

  // Outputs
  wire co;
  wire [3:0] S;

  // Instantiate the Unit Under Test (UUT)
  sum4b sim (
    .xi(xi), 
    .yi(yi), 
    .cout(cout), 
    .S(S)
  );

  initial begin
  // Initialize Inputs
    xi=0;
    for (yi = 0; yi < 16; yi = yi + 1) begin
      if (yi==0)
        xi=xi+1;
      #5 $display("el valor de %d + %d = %d", xi,yi,S) ;
    end
  end      
  
endmodule